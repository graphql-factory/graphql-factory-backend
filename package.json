{
  "name": "graphql-factory-backend",
  "version": "1.0.0",
  "description": "Backend plugin for graphql-factory",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphql-factory/graphql-factory-backend.git"
  },
  "scripts": {
    "build:copybrc": "cp build/.build.babelrc src/.babelrc",
    "build:rmbrc": "rm -f src/.babelrc",
    "example": "node example/index.js --harmony",
    "build:backend": "npm run build:copybrc && rollup -c build/rollup.backend.config.js && npm run build:rmbrc",
    "build:util": "npm run build:copybrc && rollup -c build/rollup.util.config.js && npm run build:rmbrc",
    "build": "npm run build:backend && npm run build:util",
    "test": "mocha test/index.js"
  },
  "author": "Branden Horiuchi <bhoriuchi@gmail.com>",
  "dependencies": {
    "bluebird": "^3.4.6",
    "lodash": "^4.17.2",
    "md5": "^2.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-runtime": "^6.20.0",
    "babel-register": "^6.18.0",
    "graphql": "^0.8.0",
    "graphql-factory": "git+https://github.com/graphql-factory/graphql-factory.git#development",
    "mocha": "^3.0.0",
    "rethinkdb": "^2.3.3",
    "rethinkdbdash": "^2.3.27",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^2.7.0",
    "babel-preset-es2015-rollup": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/graphql-factory/graphql-factory-backend/issues"
  },
  "homepage": "https://github.com/graphql-factory/graphql-factory-backend#readme",
  "directories": {
    "example": "example"
  },
  "keywords": [
    "graphql",
    "factory",
    "backend",
    "plugin",
    "rethinkdb"
  ]
}
